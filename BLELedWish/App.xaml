<Application x:Class="BLELedWish.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:view="clr-namespace:BLELedWish.View"
             xmlns:viewmodel="clr-namespace:BLELedWish.ViewModel"
             xmlns:model="clr-namespace:BLELedWish.Model"
             xmlns:control="clr-namespace:BLELedWish.Resource.Control"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <DataTemplate DataType="{x:Type model:MessageLED}">
            <control:MessageLEDTemplate/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodel:MessageListViewModel}">
            <view:MessageListView/>
        </DataTemplate>
    </Application.Resources>
</Application>
